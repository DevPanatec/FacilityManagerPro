# Runtime Environment
NEXT_RUNTIME=nodejs
NODE_ENV=development

# Application URLs
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=your_api_url_here
NEXT_PUBLIC_VERCEL_URL=http://localhost:3000

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# OpenAI Configuration (if needed)
# OPENAI_API_KEY=your_openai_api_key_here

# AWS S3 Configuration (if using S3)
# S3_ACCESS_KEY=your_s3_access_key_here
# S3_SECRET_KEY=your_s3_secret_key_here

# Email Configuration (if using email service)
# SENDGRID_API_KEY=your_sendgrid_api_key_here

# SMS Configuration (if using SMS service)
# SUPABASE_AUTH_SMS_TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# OAuth Providers (uncomment and configure as needed)
# SUPABASE_AUTH_EXTERNAL_APPLE_SECRET=your_apple_secret_here
# SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=your_google_secret_here
# SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET=your_github_secret_here

# Add any additional environment variables your app needs below
# NEXT_PUBLIC_FEATURE_FLAG=
# NEXT_PUBLIC_API_VERSION= 